<!DOCTYPE html>
<html>
<head>
  <title>TMC</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="row">
      <%= link_to root_path do %>
        <%= image_tag "logo.png", :class => 'logo' %>
      <% end %>
    </div>
    <%= yield %>
  </div>

</body>
</html>
